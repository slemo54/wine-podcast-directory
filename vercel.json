{
  "version": 2,
  "framework": null,
  "functions": {
    "api/index.ts": {
      "runtime": "@vercel/node@3.2.39",
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
